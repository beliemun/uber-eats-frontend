  "dependencies": {
    "@apollo/client": "^3.4.17",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/google-map-react": "^2.1.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.2",
    "autoprefixer": "^10.4.0",
    "cypress": "^9.1.1",
    "global": "^4.4.0",
    "google-map-react": "^2.1.10",
    "graphql": "^15.7.2",
    "mock-apollo-client": "^1.2.0",
    "postcss": "^8.3.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.1.2",
    "react-hook-form": "^7.20.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "subscriptions-transport-ws": "^0.11.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.1.2",
    "victory": "^36.2.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "apollo:codegen": "rm -rf src/__generated__ npx && apollo client:codegen src/__generated__ --target=typescript --outputFlat",
    "tailwind:build": "tailwind build -i ./src/styles/tailwind.css -o ./src/styles/styles.css",
    "start": "npm run tailwind:build & react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.tsx",
      "src/routers/**/*.tsx",
      "src/routes/**/*.tsx"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.2",
    "@types/multer": "^1.4.7"
  }
}
